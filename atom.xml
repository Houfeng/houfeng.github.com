layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>侯锋博客</title>
 <link href="http://houfeng.net/atom.xml" rel="self"/>
 <link href="http://houfeng.net/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://houfeng.net</id>
 <author>
   <name>houfeng</name>
   <email>admin@xhou.net</email>
 </author>

 {% for post in site.posts limit:20 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://houfeng.net{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://houfeng.net{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
